<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org// DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
 	<typeAliases>
		<typeAlias type="spring.data.HotelDto" alias="hdto"/>
		<typeAlias type="spring.data.MemberDto" alias="mbdto"/>
		<typeAlias type="spring.data.BookDto" alias="bdto"/>
		<typeAlias type="spring.data.EmpDto" alias="edto"/>
		<typeAlias type="spring.data.MenuDto" alias="mndto"/>
		<typeAlias type="spring.data.OrderDto" alias="odto"/>
		<typeAlias type="spring.data.QnaDto" alias="qdto"/>
		<typeAlias type="spring.data.QnaReplyDto" alias="qrdto"/>
		<typeAlias type="spring.data.ReviewDto" alias="rvdto"/>
		<typeAlias type="spring.data.RoomDto" alias="rmdto"/> 
		<typeAlias type="spring.data.OrderDetailDto" alias="oddto"/> 
		<typeAlias type="spring.data.FinalBookDto" alias="fbdto"/>
		

	</typeAliases>
	<mappers>
		<mapper resource="mybatis/sql/AdminSql.xml"/>
		<mapper resource="mybatis/sql/BookSql.xml"/>
		<mapper resource="mybatis/sql/MemberSql.xml"/>		
		<mapper resource="mybatis/sql/MenuSql.xml"/>
		<mapper resource="mybatis/sql/OrderSql.xml"/>
		<mapper resource="mybatis/sql/QnaSql.xml"/>
		<mapper resource="mybatis/sql/ReviewSql.xml"/> 
		
	</mappers>
</configuration>

